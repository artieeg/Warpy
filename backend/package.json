{
  "name": "backend_2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:nodemon2": "nest start --watch --exec 'node -r tsconfig-paths/register -r ts-node/register --inspect=0.0.0.0 ./src/main.ts'",
    "start:nodemon": "ts-node-dev -r tsconfig-paths/register ./src/main.ts --watch ./src --exit-child",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/microservices": "^8.0.9",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "2.29.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@warpy/lib": "1.0.0",
    "axios": "^0.22.0",
    "bad-words": "^3.0.4",
    "cuid": "^2.1.8",
    "jsonwebtoken": "^8.5.1",
    "nats": "^2.2.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.9",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/jest-when": "^2.7.3",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "jest-when": "^3.4.1",
    "prettier": "^2.3.2",
    "prisma": "2.29.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "preset": "ts-jest",
    "timers": "fake",
    "moduleNameMapper": {
      "^@backend_2/(.*)$": "<rootDir>/$1"
    },
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.(entity|controller|module|cache).(t|j)s",
      "!**/prisma.service.ts",
      "!**/nats.service.ts",
      "!**/app.service.ts",
      "!**/main.ts",
      "!**/errors.ts",
      "!**/nats.transporter.ts",
      "!**/rpc-exception.filter.ts",
      "!**/nats"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
